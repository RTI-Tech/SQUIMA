{
  "version": 2,
  "builds": [
    {
      "src": "flet --w --host 0.0.0.0 --p 80",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.11"}
    },
    {
      "src": "bluild_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "static\(.*)",
      "dest": "static\$1"
    },
    {
      "src": "/(.*)",
      "dest": "squima/main.py
    }
  ]
}
